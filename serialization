import pickle
import tile


# pickle can only save one file - otherwise use shelve

outFile = open('pickle.txt', 'wb')
pickle.dump(tile.outdoor, outFile)
outFile.close()

inFile = open('pickle.txt', 'rb')
newList = pickle.load(inFile)
print (newList)
